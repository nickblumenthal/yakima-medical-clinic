{
  "name": "yakima_medical_clinic",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.0.0-beta",
    "node-sass-chokidar": "0.0.3",
    "react": "^16.0.0",
    "react-burger-menu": "^2.1.9",
    "react-dom": "^16.0.0",
    "react-icons": "^3.10.0",
    "react-responsive": "^3.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "reactstrap": "^5.0.0-alpha.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "NODE_PATH=src:src/components:src/containers:src/images react-scripts start",
    "build": "NODE_PATH=src:src/components:src/containers:src/images react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
